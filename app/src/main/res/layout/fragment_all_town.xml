<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.NestedScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >
<LinearLayout
  android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/dp_10"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_admin_main"
        android:background="@color/white"
        >
        <TextView
            android:id="@+id/tv_all_city_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/str_admin_all_town"
            android:layout_marginLeft="@dimen/dp_20"
            android:layout_centerVertical="true"
            android:textSize="@dimen/sp_admin_main_02"
            android:textStyle="bold"
            android:textColor="@color/admin_main_select_text"
            />
        <Spinner
            android:id="@+id/id_spinner_year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toLeftOf="@+id/id_spinner_all"
            >
        </Spinner>

        <Spinner
            android:id="@+id/id_spinner_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="@dimen/dp_10"
            >
        </Spinner>
    </RelativeLayout>
    <com.threehmis.bjaj.widget.ExpandableGridView
        android:id="@+id/gridview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:columnWidth="90dp"
        android:stretchMode="spacingWidthUniform"
        android:horizontalSpacing="@dimen/dp_4"
        android:verticalSpacing="@dimen/dp_20"
        android:layout_marginTop="@dimen/dp_10"
        android:numColumns="3"
        android:layout_marginBottom="@dimen/dp_10"
        />
    <com.threehmis.bjaj.widget.chartview.BarChart02View
        android:id="@+id/mHorizontalBarChart"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_400"
        />
</LinearLayout>
</android.support.v4.widget.NestedScrollView>